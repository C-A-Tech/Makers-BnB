<h1>Create a new listing</h1>
<ul>
  <form action="/space" method="post">
    <li>  Title <input type="text" name="title"> </li>
    <li>  Description <input type="text" name="description"> </li>
    <li>  Location <input type="text" name="location"> </li>
    <li>  Price <input type="text" name="price"> </li>
    <label for="start"></label>
    <input type="date" id="start" name="available_from"
     min="2021-04-01" max="2025-04-29">
    <br>
     <label for="end"> to:</label>
     <br>
     <input type="date" id="end" name="available_to"
     min="2021-04-02" max="2025-04-30">
    <p>space for dates</p>
    <input type="submit" name="Submit">
  </form>
</ul>